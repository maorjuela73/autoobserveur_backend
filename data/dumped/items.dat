INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (1, 'C1', 'Concepciones - Percepciones', 'Características de un estudiante “efectivo”', 'Soy un estudiante efectivo', 'Me considero un estudiante efectivo, teniendo en cuenta que implemento estrategias cognitivas y sociales útiles y pertinentes para mis procesos de repaso, toma de apuntes y argumentación en mis clases, en el marco de mis propios objetivos de aprendizaje.', true, '2019-04-15 06:42:41.561128', '2019-04-15 06:42:41.561128');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (2, 'C2', 'Concepciones - Percepciones', 'Características de un estudiante “efectivo”', 'Reflexiono sobre mi aprendizaje', 'Hago procesos de reflexión y evaluación de mis estrategias de aprendizaje (recopilando la mayor cantidad de información disponible), para establecer su nivel de efectividad; de esta forma, puedo identificar qué estrategias sigo implementando, cuáles estrategias deben ser cambiadas e identifico cuáles estrategias necesito aprender.', true, '2019-04-15 06:42:41.572096', '2019-04-15 06:42:41.572096');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (3, 'C3', 'Concepciones - Percepciones', 'Valoración del grado de dificultad que me implica un nuevo aprendizaje', 'Soy consciente de cuándo un aprendizaje es fácil o difícil', 'Tengo criterios claros para establecer si un proceso de aprendizaje me resulta fácil o implica un nivel alto de complejidad, con lo que tomo una decisión sobre uso de estrategias cognitivas y sociales de aprendizaje.', true, '2019-04-15 06:42:41.577085', '2019-04-15 06:42:41.577085');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (4, 'C4', 'Concepciones - Percepciones', 'Estilos de aprendizaje', 'Comprendo mis estilos de aprendizaje', 'He identificado los estilos de aprendizaje que predominan en mí y logro hacerlos compatibles con los estilos de enseñanza de mis profesores y compañeros de curso.', false, '2019-04-15 06:42:41.584065', '2019-04-15 06:42:41.584065');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (5, 'A1', 'Acciones', 'Uso eficaz del tiempo individual de trabajo', 'Programo de manera eficiente las sesiones de trabajo individual', 'Administro todos los recursos necesarios para mis sesiones de trabajo individual concentrado (horarios de trabajo y horarios de descanso, valoración para la distribución de distintos tiempos para distintas tareas, materiales, etc.).', true, '2019-04-15 06:42:41.590049', '2019-04-15 06:42:41.590049');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (6, 'A2', 'Acciones', 'Uso eficaz del tiempo individual de trabajo', 'Establezco resultados finales esperados', 'Identifico los resultados esperados de cada sesión de trabajo individual.', true, '2019-04-15 06:42:41.595035', '2019-04-15 06:42:41.595035');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (7, 'A3', 'Acciones', 'Uso eficaz del tiempo individual de trabajo', 'Reacciono ante las distracciones', 'Tomo acciones de mejora para retomar mi centro de atención en el trabajo individual, cuando identifico posibles distracciones.', true, '2019-04-15 06:42:41.602017', '2019-04-15 06:42:41.602017');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (8, 'A4', 'Acciones', 'Estrategias para recolección de información en clase y lecturas', 'Agrupo insumos de calidad', 'Uso apropiadamente los contenidos escogidos en mi preparación de la clase y desarrollados durante ella.', false, '2019-04-15 06:42:41.607011', '2019-04-15 06:42:41.607011');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (9, 'A5', 'Acciones', 'Planes de acción', 'Creo planes de acción', 'Establezco planes de acción a corto y mediano plazo, que son pertinentes para cumplir con la mayoría de mis responsabilidades.', true, '2019-04-15 06:42:41.610993', '2019-04-15 06:42:41.610993');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (10, 'A6', 'Acciones', 'Planes de acción', 'Identifico la diferencia entre tareas urgentes e importantes', 'Establezco la diferencia entre las tareas urgentes y las tareas importantes que debo abordar y las vinculo a mis planes de acción.', false, '2019-04-15 06:42:41.616977', '2019-04-15 06:42:41.616977');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (11, 'A7', 'Acciones', 'Estrategias efectivas para el descanso', 'Administro mis jornadas de sueño', 'Respeto mis horas de sueño, en condiciones apropiadas para el descanso (más de 7 horas por día).', false, '2019-04-15 06:42:41.623958', '2019-04-15 06:42:41.623958');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (12, 'A8', 'Acciones', 'Estrategias efectivas para el descanso', 'Hago alguna actividad física', 'Realizo alguna práctica deportiva, por al menos 30 minutos diarios.', false, '2019-04-15 06:42:41.627947', '2019-04-15 06:42:41.627947');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (13, 'A9', 'Acciones', 'Estrategias efectivas para el descanso', 'Cuido mi nutrición', 'Me alimento apropiadamente (consumo agua, frutas y refrigerios saludables, etc.).', false, '2019-04-15 06:42:41.632954', '2019-04-15 06:42:41.632954');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (14, 'A10', 'Acciones', 'Estrategias efectivas para el descanso', 'Escojo espacios adecuados para el estudio', 'Desarrollo mis sesiones de trabajo académico (individual o grupal) en espacios sanos (libres de humor, libres de ruido, con buena iluminación, con sillas y mesas cómodas, etc.).', false, '2019-04-15 06:42:41.635938', '2019-04-15 06:42:41.635938');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (15, 'A11', 'Acciones', 'Estrategias efectivas para el descanso', 'Realizo pausas activas', 'En mis descansos, realizo actividades relajantes como escuchar música, desarrollar alguna actividad lúdica, ejercicios de respiración y relajación, etc.', false, '2019-04-15 06:42:41.639915', '2019-04-15 06:42:41.639915');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (16, 'A12', 'Acciones', 'Estrategias efectivas para el descanso', 'Consulto a mi cuerpo para tomar decisiones', 'Estudio cuando me encuentro en condiciones físicas óptimas (tengo buen estado de salud general, estoy fresco, me siento bien, etc.) y además tengo prácticas saludables de sueño y nutrición.', true, '2019-04-15 06:42:41.643906', '2019-04-15 06:42:41.643906');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (17, 'A13', 'Acciones', 'Estilos de aprendizaje y estrategias de trabajo grupal ', 'Programo de manera eficiente las sesiones de trabajo grupal', 'Administro todos los recursos necesarios para mis sesiones de trabajo grupal (horarios de trabajo y horarios de descanso, valoración para la distribución de distintos tiempos para distintas tareas, materiales, resultados, apoyo de otros, etc.).', true, '2019-04-15 06:42:41.649894', '2019-04-15 06:42:41.649894');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (18, 'A14', 'Acciones', 'Estilos de aprendizaje y estrategias de trabajo grupal ', 'Respeto la opinión del otro en la toma de decisiones colectiva', 'En nuestro grupo, tomamos decisiones sobre el trabajo académico considerando la opinión de todos los integrantes.', false, '2019-04-15 06:42:41.654876', '2019-04-15 06:42:41.654876');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (19, 'A15', 'Acciones', 'Estilos de aprendizaje y estrategias de trabajo grupal ', 'Comunico el alcance de mi trabajo autónomo y lo conecto con el de los demás', 'Cada integrante del grupo se compromete con tareas específicas y reporta a los demás integrantes su avance y desarrollo.', false, '2019-04-15 06:42:41.658864', '2019-04-15 06:42:41.658864');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (20, 'M1', 'Motivaciones', 'Razones para desarrollar mi trabajo académico', 'Escojo libremente las asignaturas que inscribo', 'Tengo claras razones personales (intrínsecas y extrínsecas) que distan de las obligatorias para la inscripción de mis cursos en la universidad.', false, '2019-04-15 06:42:41.66386', '2019-04-15 06:42:41.66386');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (21, 'M2', 'Motivaciones', 'Razones para desarrollar mi trabajo académico', 'Reviso mis niveles de motivación', 'Evalúo mis niveles de motivación respecto a un curso o una tarea académica, genero alertas y tomo acciones de ser necesarias.', true, '2019-04-15 06:42:41.670833', '2019-04-15 06:42:41.670833');
INSERT INTO public.items (id, code, function, dimension, name, description, is_active, created_at, updated_at) VALUES (22, 'M3', 'Motivaciones', 'Razones para desarrollar mi trabajo académico', 'Comunico mi evaluación de motivaciones', 'Comparto con mis profesores la evolución de mis motivaciones respecto a la(s) asignatura(s).', true, '2019-04-15 06:42:41.676829', '2019-04-15 06:42:41.676829');
